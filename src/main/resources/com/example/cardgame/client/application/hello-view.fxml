                                           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                                         </content>
                                       </ScrollPane>
                                    </children>
                                 </AnchorPane>
                             </items>
                           </SplitPane>
                        </children>
                     </AnchorPane>
                 </content>
               </ScrollPane>
            </children></AnchorPane>
         <AnchorPane prefHeight="200.0" prefWidth="200.0" />
      </children>
   </VBox>

  <Label fx:id="welcomeText" />
  <Button onAction="#handleSearch" text="Hello!" />
</VBox>
