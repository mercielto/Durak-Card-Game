<VBox alignment="CENTER" prefHeight="305.0" prefWidth="234.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cardgame.client.controller.MainController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <VBox prefHeight="200.0" prefWidth="100.0">
      <children>
         <AnchorPane prefHeight="200.0" prefWidth="200.0" />
         <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
               <ScrollPane layoutY="-14.0" prefHeight="200.0" prefWidth="200.0">
